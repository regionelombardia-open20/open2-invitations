{
  "name": "lispa/amos-invitations",
  "description": "System to invite persons to platform",
  "keywords": ["amos", "yii2", "invitations"],
  "homepage": "http://www.lispa.it/",
  "type": "component",
  "proscription": "BSD-3-Clause",
  "require": {
    "php": ">=5.4.0",
    "lispa/amos-core" : "*",
    "lispa/amos-dashboard" : "*",
    "lispa/amos-admin" : "*",
    "lispa/amos-notify" : "*"
  },
  "config": {
    "process-timeout": 1800,
    "secure-http": false
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "lispa\\amos\\invitations\\": "src"
    }
  },
  "require-dev": {
    "yiisoft/yii2-faker": "*"
  }
}